PORT=3001
MONGODB_URI=mongodb://127.0.0.1:27017/farmease

# Plant.id API keys (any of these formats work; placeholders below)
# Preferred: comma-separated list
PLANT_ID_API_KEYS=PLANTID_KEY_1,PLANTID_KEY_2
# Or indexed variants (1..N)
# PLANT_ID_API_KEY_1=PLANTID_KEY_1
# PLANT_ID_API_KEY_2=PLANTID_KEY_2
# Or single key (fallback if above not set)
# PLANT_ID_API_KEY=PLANTID_SINGLE_KEY

# Kindwise API keys
KINDWISE_API_KEYS=KINDWISE_KEY_1,KINDWISE_KEY_2
# KINDWISE_API_KEY_1=KINDWISE_KEY_1
# KINDWISE_API_KEY_2=KINDWISE_KEY_2
# KINDWISE_API_KEY=KINDWISE_SINGLE_KEY

# Groq AI API keys for crop recommendations
# Preferred: comma-separated list
GROQ_API_KEYS=gsk_YOUR_GROQ_KEY_1,gsk_YOUR_GROQ_KEY_2,gsk_YOUR_GROQ_KEY_3
# Or indexed variants (1..N)
# GROQ_API_KEY_1=gsk_YOUR_GROQ_KEY_1
# GROQ_API_KEY_2=gsk_YOUR_GROQ_KEY_2
# GROQ_API_KEY_3=gsk_YOUR_GROQ_KEY_3
# Or single key (fallback if above not set)
# GROQ_API_KEY=gsk_YOUR_GROQ_SINGLE_KEY
